                                                                              fieldName="Lightning_Step__First_Name__c"
                                                                              aura:id="Lightning_Step__First_Name__c"
                                                                              required="true"
                                                                              /> 
                                                    </div>                             
                                                </div>
                                                <div class="slds-grid">
                                                    <div class="slds-col slds-size_1-of-2">                                 
