            <div class="slds-modal__container slds-modal_small callLeadInqCmpOne">
                <header class="slds-modal__header slds-theme_error slds-theme_alert-texture">
                    <h2 class="slds-text-heading_medium" id="prompt-heading-id">Message Alert</h2>
                </header>
                <div class="slds-modal__content slds-p-around_medium" id="prompt-message-wrapper">
                    <p class="slds-text-align_center slds-text-heading_small">{!v.ErrorMessage}</p>
                </div>
            </div>
        </div>
    </aura:if>
