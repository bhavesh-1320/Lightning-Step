<aura:component controller="CreateInquiryController"  implements="force:lightningQuickActionWithoutHeader,force:hasRecordId,force:hasSObjectName,flexipage:availableForAllPageTypes,force:appHostable" access="global" >
    
    <aura:attribute name="showSpinner" type="Boolean" default="false" />
    <aura:attribute name="leadRecord" type="Lead"/>
    <aura:attribute name="myDate" type="date" />
    <aura:attribute name="ErrorMessage" type="string" />
    <aura:attribute name="showForm" type="boolean" default="false"/>
    <aura:attribute name="DOBSection" type="boolean" default="false"/>
    <aura:attribute name="showError" type="boolean" default="false"/>
    <aura:attribute name="submissionDatefield" type="date"/>
    <aura:attribute name="DOB" type="date"/>
    <aura:attribute name="activeSections" type="List" default="['Personal Information', 'Caller Information', 'Payor Details','Address', 'Insurance Information', 'Case Results', 'Insured Information', 'Check All', 'Additional Personal Information']" />
    <aura:attribute name="dateValidationError" type="boolean" />
    <aura:attribute name="insOptions" type="String"/>
    <aura:attribute name="selectedValue" type="Integer"/>
    <aura:attribute name="fundSource" type="String"/>
    <aura:attribute name="alternateName" type="Boolean"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="sObjectName" type="String"/>
    <aura:attribute name = "recordId" type="Id"/>
    
    <ltng:require styles="{!$Resource.LeadInquiryComponent}" /> 
    
